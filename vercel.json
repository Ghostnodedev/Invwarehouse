{
  "version": 2,
  "builds": [
    {
      "src": "src/inv.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/inventory",
      "methods": ["GET"],
      "dest": "/src/inv.js"
    },
    {
      "src": "/postprisma",
      "methods": ["POST"],
      "dest": "/src/inv.js"
    },
    {
      "src": "/deletefromdb",
      "methods": ["DELETE"],
      "dest": "/src/inv.js"
    }
  ]
}
